{
  "version": 2,
  "rewrites": [
    {
      "source": "/waitlist",
      "destination": "https://waitlist-mm-website.vercel.app"
    },
    {
      "source": "/waitlist/:path*",
      "destination": "https://waitlist-mm-website.vercel.app/:path*"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/renewal-reminders",
      "schedule": "0 9 * * *"
    }
  ]
}
